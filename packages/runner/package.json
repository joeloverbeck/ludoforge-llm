{
  "name": "@ludoforge/runner",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "bootstrap:fixtures": "pnpm -F @ludoforge/engine build && node scripts/bootstrap-fixtures.mjs generate",
    "bootstrap:fixtures:check": "pnpm -F @ludoforge/engine build && node scripts/bootstrap-fixtures.mjs check",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.6",
    "@ludoforge/engine": "workspace:*",
    "@pixi/react": "^8.0.0",
    "comlink": "^4.4.2",
    "dexie": "^4.3.0",
    "graphology": "^0.26.0",
    "graphology-layout-forceatlas2": "^0.10.1",
    "gsap": "^3.14.2",
    "pixi-viewport": "^6.0.1",
    "pixi.js": "^8.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.1.4",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^28.1.0",
    "typescript": "^5.9.2",
    "vite": "^7.0.0",
    "vitest": "^4.0.18",
    "yaml": "^2.8.2"
  }
}
